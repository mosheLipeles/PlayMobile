{
  "expo": {
    "name": "PlayMobile",
    "slug": "PlayMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/iconP.jpg",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "PlayMobile is a small application that contain a simple game - Rock, paper, scissors."
  }
}
